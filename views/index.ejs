    <style>
        @keyframes float { 0%, 100% { transform: translateY(0); } 50% { transform: translateY(-10px); } }
        @keyframes fadeInUp { from { opacity: 0; transform: translateY(10px); } to { opacity: 1; transform: translateY(0); } }
        @keyframes twinkle { 0% { opacity: 0.7; } 50% { opacity: 0.9; } 100% { opacity: 0.7; } }
        @keyframes aurora { 0% { transform: translateX(-100%); } 100% { transform: translateX(100%); } }
        @keyframes orbit { 0% { transform: rotate(0deg) translateX(50px) rotate(0deg); } 100% { transform: rotate(360deg) translateX(50px) rotate(-360deg); } }
        @keyframes orbit {from { transform: rotate(0deg); }to { transform: rotate(360deg); }}
        .cosmic-bg { background: linear-gradient(to bottom, #191970, #000000); }
        .glass-card { background: rgba(255, 255, 255, 0.1); backdrop-filter: blur(10px); }
        .stars { position: fixed; top: 0; left: 0; width: 100%; height: 100%; pointer-events: none; background-image: radial-gradient(2px 2px at 20px 30px, #eee, rgba(0,0,0,0)), radial-gradient(2px 2px at 40px 70px, #fff, rgba(0,0,0,0)), radial-gradient(2px 2px at 50px 160px, #ddd, rgba(0,0,0,0)), radial-gradient(2px 2px at 90px 40px, #fff, rgba(0,0,0,0)), radial-gradient(2px 2px at 130px 80px, #fff, rgba(0,0,0,0)); background-repeat: repeat; background-size: 200px 200px; animation: twinkle 5s infinite; }
        .aurora { position: absolute; top: 0; left: 0; right: 0; height: 50px; background: linear-gradient(to right, #00ff00, #00ffff, #ff00ff); opacity: 0.3; filter: blur(30px); animation: aurora 20s linear infinite; }
        .timeline-container { position: relative; }
        .timeline-container::before { content: ''; position: absolute; top: 0; left: 50%; width: 2px; height: 100%; background: linear-gradient(to bottom, transparent, #00f7ff, transparent); transform: translateX(-50%); }
        .timeline-item { position: relative; margin-bottom: 50px; }
        .timeline-item::before { content: ''; position: absolute; left: 50%; top: 50%; width: 20px; height: 20px; background-color: #00f7ff; border-radius: 50%; transform: translate(-50%, -50%); }
        .timeline-content { width: 45%; }
        .timeline-item:nth-child(odd) .timeline-content { margin-left: auto; }
        .cosmic-separator { height: 2px; background: linear-gradient(to right, transparent, #00f7ff, transparent); margin: 40px 0; }
        .testimonial-card { background: rgba(255, 255, 255, 0.1); backdrop-filter: blur(10px); border-radius: 10px; padding: 20px; margin: 10px; transition: all 0.3s ease; }
        .testimonial-card:hover { transform: translateY(-5px); box-shadow: 0 10px 20px rgba(0, 230, 255, 0.2); }
        .testimonial-icon { font-size: 24px; margin-bottom: 10px; }
        .swiper-pagination-bullet { background: #00f7ff; }
        .pricing-card { transition: all 0.3s ease; }
        .pricing-card:hover { transform: translateY(-10px) scale(1.05); }
        .pricing-icon { font-size: 48px; margin-bottom: 20px; }
        .price-tag { font-size: 36px; font-weight: bold; margin: 20px 0; }
        .feature-list li { margin-bottom: 10px; }
        .planet { width: 100px; height: 100px; border-radius: 50%; position: absolute; }
        .planet-1 { background: radial-gradient(circle at 30% 30%, #FFB6C1, #FF69B4); top: 10%; left: 5%; animation: orbit 20s linear infinite; }
        .planet-2 { background: radial-gradient(circle at 30% 30%, #87CEFA, #1E90FF); bottom: 15%; right: 10%; animation: orbit 25s linear infinite reverse; }
        .integration-card {
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
            opacity: 0;
            transform: translateY(20px);
            margin-bottom: 2rem;
        }
        .integration-card.aos-animate {
            opacity: 1;
            transform: translateY(0);
        }
        .integration-card::before {
            content: '';
            position: absolute;
            top: -50%;
            left: -50%;
            width: 200%;
            height: 200%;
            background: radial-gradient(circle, rgba(255,255,255,0.1) 0%, rgba(255,255,255,0) 70%);
            transform: rotate(45deg);
            transition: all 0.3s ease;
        }
        .integration-card:hover::before {
            top: -75%;
            left: -75%;
        }
        .integration-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 30px rgba(0, 230, 255, 0.2);
        }
        .integration-icon {
            transition: all 0.3s ease;
        }
        .integration-card:hover .integration-icon {
            transform: scale(1.1) rotate(5deg);
        }
        .tab-content {
            display: none;
        }
        .tab-content.active {
            display: grid;
        }
        .tab-btn {
            opacity: 0.7;
            transition: all 0.3s ease;
        }
        .tab-btn.active {
            opacity: 1;
            box-shadow: 0 0 15px rgba(0, 230, 255, 0.5);
        }
    </style>
</head>
